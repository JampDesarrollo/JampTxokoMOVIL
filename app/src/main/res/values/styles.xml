<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorJAMP</item>
        <item name="colorPrimaryDark">@color/colorJAMP</item>
        <item name="colorAccent">@color/colorJAMP</item>
        <item name="android:textColorPrimary">@color/blanco</item>
        <item name="android:navigationBarColor">@color/colorJAMP</item>
        <item name="android:colorForeground">@android:color/white</item>
        <item name="android:textColorPrimaryInverse">@android:color/white</item>
        <item name="android:colorBackground">@color/colorJAMP</item>
        <item name="android:textColorSecondaryInverse">@color/blanco</item>

    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="SplashTheme" parent="AppTheme">
        <item name="android:windowBackground">@drawable/splash</item>
    </style>

    <style name="Dialog_custom" parent="AppTheme">
        <item name="android:textColorPrimary">#595959</item>
    </style>

</resources>
